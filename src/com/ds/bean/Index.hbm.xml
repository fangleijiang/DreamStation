<?xml version="1.0" encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="com.ds.bean.Index" table="index_info">
		<id name="id" column="id" type="int">
			<generator class="increment" />
		</id>
		<property name="pic_name" column="pic_name" type="string"></property>
		<property name="pic_description" column="pic_description" type="string"></property>
		<property name="pic_address" column="pic_address" type="string"></property>

	</class>
</hibernate-mapping>